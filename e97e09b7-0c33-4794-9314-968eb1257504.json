[ {
  "defaultTab" : "nodes",
  "description" : "CNC - List Alerts\n# Overview\nList all the current alerts",
  "executionEnabled" : true,
  "group" : "CNC/Services",
  "id" : "e97e09b7-0c33-4794-9314-968eb1257504",
  "loglevel" : "INFO",
  "name" : "CNC - List Alerts",
  "nodeFilterEditable" : true,
  "notification" : {
    "onstart" : {
      "email" : {
        "recipients" : "siprice@cisco.com",
        "subject" : "siprice@cisco.com"
      }
    },
    "onsuccess" : {
      "email" : {
        "attachLog" : true,
        "attachLogInFile" : true,
        "recipients" : "device-details@necehealthpoc.local",
        "subject" : "${notification.eventStatus} ${job.name}"
      }
    }
  },
  "notifyAvgDurationThreshold" : null,
  "options" : [ {
    "enforced" : true,
    "label" : "Crosswork Deployment VIP",
    "name" : "cnc-pod",
    "required" : true,
    "valuesUrl" : "file:/opt/cw/options/cw-environments.json"
  }, {
    "label" : "Crosswork Admin User",
    "name" : "cw-user",
    "required" : true,
    "value" : "admin",
    "values" : [ "admin" ],
    "valuesListDelimiter" : ","
  }, {
    "label" : "Crosswork Admin password",
    "name" : "cw-password",
    "required" : true,
    "secure" : true,
    "storagePath" : "keys/cw/admin",
    "valueExposed" : true
  } ],
  "plugins" : {
    "ExecutionLifecycle" : null
  },
  "scheduleEnabled" : true,
  "sequence" : {
    "commands" : [ {
      "description" : "get vars",
      "jobref" : {
        "group" : "CNC/Configuration",
        "importOptions" : true,
        "name" : "get key vars",
        "uuid" : "c3b0db66-3fdd-49b7-a5b5-4261a0ff3463"
      }
    }, {
      "configuration" : {
        "authentication" : "None",
        "body" : "{\"openAlarmsOnly\":true,\"criteria\":\"select * from alarm limit 100 page 0 where alarmCategory=1 \"}",
        "checkResponseCode" : "false",
        "file" : "/opt/cw/tmp/${job.execid}.json",
        "headers" : "{'Authorization': 'Bearer ${export.cwtoken}','Content-Type': 'application/json'}",
        "method" : "POST",
        "printResponse" : "false",
        "printResponseToFile" : "true",
        "proxySettings" : "false",
        "remoteUrl" : "https://${export.cwvip}:30603/crosswork/alarms/v1/query",
        "sslVerify" : "false",
        "timeout" : "30000"
      },
      "description" : "GetEvents",
      "nodeStep" : false,
      "type" : "edu.ohio.ais.rundeck.HttpWorkflowStepPlugin"
    }, {
      "configuration" : {
        "authentication" : "None",
        "body" : "{\"time_ago\":\"0m\",\"offset\":\"0\",\"time_interval\":\"1h\",\"levels\":[\"CRITICAL\",\"MAJOR\",\"WARNING\",\"MINOR\",\"INFO\"],\"limit\":\"200\",\"top_devices\":true}",
        "checkResponseCode" : "false",
        "file" : "/opt/cw/tmp/${job.id}-devices.json",
        "headers" : "{'Authorization': 'Bearer ${export.cwtoken}','Content-Type': 'application/json'}",
        "method" : "POST",
        "printResponse" : "false",
        "printResponseToFile" : "true",
        "proxySettings" : "false",
        "remoteUrl" : "https://${export.cwvip}:30603/crosswork/hi/v1/alerts/device/devices",
        "sslVerify" : "false",
        "timeout" : "30000"
      },
      "description" : "GetDeviceAlarms",
      "nodeStep" : false,
      "type" : "edu.ohio.ais.rundeck.HttpWorkflowStepPlugin"
    }, {
      "configuration" : {
        "command" : "jq '.alarms[] | select(.State==\"Critical\") |[{\"object_description\",\"State\",\"event_type\",\"Description\"}]' /opt/cw/tmp/${job.execid}.json"
      },
      "description" : "Critical Events",
      "nodeStep" : true,
      "plugins" : {
        "LogFilter" : [ {
          "config" : {
            "datatype" : "application/json",
            "sanitizeHtml" : "true",
            "striped" : "true"
          },
          "type" : "render-datatype"
        } ]
      },
      "type" : "localexec"
    }, {
      "configuration" : {
        "command" : "jq '.alarms[] | select(.State==\"Major\") |[{\"object_description\",\"State\",\"event_type\",\"Description\"}]' /opt/cw/tmp/${job.execid}.json"
      },
      "description" : "Major Events",
      "nodeStep" : true,
      "plugins" : {
        "LogFilter" : [ {
          "config" : {
            "datatype" : "application/json",
            "sanitizeHtml" : "true",
            "striped" : "true"
          },
          "type" : "render-datatype"
        } ]
      },
      "type" : "localexec"
    }, {
      "configuration" : {
        "command" : "jq '.alarms[] | select(.State==\"Minor\") |[{\"object_description\",\"State\",\"event_type\",\"Description\"}]' /opt/cw/tmp/${job.execid}.json"
      },
      "description" : "Minor Events",
      "nodeStep" : true,
      "plugins" : {
        "LogFilter" : [ {
          "config" : {
            "datatype" : "application/json",
            "sanitizeHtml" : "true",
            "striped" : "true"
          },
          "type" : "render-datatype"
        } ]
      },
      "type" : "localexec"
    } ],
    "keepgoing" : false,
    "strategy" : "node-first"
  },
  "uuid" : "e97e09b7-0c33-4794-9314-968eb1257504"
} ]
