    }, {
      "description" : "get CSV",
      "script" : "file=/opt/cw/tmp/@job.execid@.json\n#jq -r '.lsps[]| [ keys[] as $k | .[$k] ]| @csv' $file\n\njq -r '[.lsps[]  |[ keys[] as $k | .[$k]]] | [[\"color\",\"head-end\",\"tail-end\",\"id\",\"lsp-type\",\"metric-type\",\"change-count\",\"path-change-count\",\"total\"]]+. | .[] | @csv ' $file"
    } ],
    "keepgoing" : false,
    "strategy" : "node-first"
  },
  "uuid" : "cd3f2654-4e01-45e6-a197-792241262761"
