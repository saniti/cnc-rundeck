[ {
  "defaultTab" : "nodes",
  "description" : "Show application versions\n\n# Overview\n\n",
  "executionEnabled" : false,
  "group" : "CNC/Update",
  "id" : "eec06582-cb1f-4fce-bbdf-a5819aaf5ce7",
  "loglevel" : "INFO",
  "name" : "CNC-01 - Show App Versions",
  "nodeFilterEditable" : true,
  "options" : [ {
    "enforced" : true,
    "label" : "VIP for CNC",
    "name" : "cnc-vip",
    "required" : true,
    "valuesUrl" : "file:/opt/cw/options/cw-environments.json"
  }, {
    "name" : "cw-password",
    "required" : true,
    "secure" : true,
    "storagePath" : "keys/cw/cw-admin",
    "valueExposed" : true
  }, {
    "hidden" : true,
    "name" : "cw-port",
    "required" : true,
    "value" : "30603"
  } ],
  "plugins" : {
    "ExecutionLifecycle" : null
  },
  "scheduleEnabled" : true,
  "sequence" : {
    "commands" : [ {
      "description" : "Auth",
      "jobref" : {
        "args" : "-cnc-vip ${option.cnc-vip} -cnc-password ${option.cw-password}",
        "failOnDisable" : true,
        "group" : "CNC",
        "name" : "CNC - Auth",
        "uuid" : "5fee437d-4fa6-4728-9601-0fac3a061ca9"
      }
    }, {
      "configuration" : {
        "authentication" : "None",
        "checkResponseCode" : "false",
        "file" : "/opt/cw/tmp/${job.id}-appversions.log",
        "headers" : "{\"Content-Type\": \"application/json\",\"Authorization\": \"Bearer ${export.cnc-token}\"}",
        "method" : "POST",
        "printResponse" : "true",
        "printResponseToFile" : "true",
        "proxySettings" : "false",
        "remoteUrl" : "https://${option.cnc-vip}:30603/crosswork/platform/v2/capp/applicationsummary/query",
        "sslVerify" : "false",
        "timeout" : "30000"
      },
      "description" : "Get app versions",
      "nodeStep" : false,
      "type" : "edu.ohio.ais.rundeck.HttpWorkflowStepPlugin"
    }, {
      "configuration" : {
        "debugOnly" : "false"
      },
      "description" : "log data",
      "nodeStep" : false,
      "type" : "log-data-step"
    }, {
      "configuration" : {
        "command" : "jq '[.application_summary_list[].application_data | (.[\"Module\"] =.summary.name)|(.[\"Version\"] =.version) | {Module,Version}]' /opt/cw/tmp/${job.id}-appversions.log"
      },
      "nodeStep" : true,
      "plugins" : {
        "LogFilter" : [ {
          "config" : {
            "invalidKeyPattern" : "\\s|\\$|\\{|\\}|\\\\",
            "logData" : "true",
            "name" : "appdata",
            "regex" : "(.*)"
          },
          "type" : "key-value-data"
        } ]
      },
      "type" : "localexec"
    }, {
      "configuration" : {
        "command" : "echo ${data.appdata*}"
      },
      "description" : "render",
      "nodeStep" : true,
      "plugins" : {
        "LogFilter" : [ {
          "config" : {
            "datatype" : "application/json",
            "sanitizeHtml" : "true",
            "striped" : "true"
          },
          "type" : "render-datatype"
        } ]
      },
      "type" : "localexec"
    } ],
    "keepgoing" : false,
    "strategy" : "node-first"
  },
  "uuid" : "eec06582-cb1f-4fce-bbdf-a5819aaf5ce7"
} ]
