[ {
  "defaultTab" : "nodes",
  "description" : "",
  "executionEnabled" : true,
  "group" : "CNC/Configuration",
  "id" : "801e3747-5afc-4a08-861f-92306ce08eba",
  "loglevel" : "INFO",
  "name" : "CNC - Create Tags",
  "nodeFilterEditable" : false,
  "options" : [ {
    "enforced" : true,
    "label" : "CNC POD",
    "name" : "cnc-pod",
    "required" : true,
    "valuesUrl" : "file:/opt/cw/options/cw-environments.json"
  }, {
    "description" : "> Enter Category Name for TAG",
    "label" : "Tag Category",
    "name" : "tag-category",
    "required" : true
  }, {
    "description" : "> Enter TAG name (one only)",
    "label" : "New tag to be added",
    "name" : "tag",
    "required" : true
  } ],
  "plugins" : {
    "ExecutionLifecycle" : null
  },
  "scheduleEnabled" : true,
  "sequence" : {
    "commands" : [ {
      "configuration" : {
        "command" : "echo {\"tags\":[{\"name\":\"${option.tag}\",\"category\":\"${option.tag-category}\"}]}\""
      },
      "description" : "create payload",
      "nodeStep" : true,
      "plugins" : {
        "LogFilter" : [ {
          "config" : {
            "invalidKeyPattern" : "\\s|\\$|\\{|\\}|\\\\",
            "logData" : "true",
            "name" : "tag",
            "regex" : "(.*)"
          },
          "type" : "key-value-data"
        } ]
      },
      "type" : "localexec"
    }, {
      "description" : "get vars",
      "jobref" : {
        "group" : "CNC/Configuration",
        "importOptions" : true,
        "name" : "get key vars",
        "uuid" : "c3b0db66-3fdd-49b7-a5b5-4261a0ff3463"
      }
    }, {
      "configuration" : {
        "authentication" : "None",
        "body" : "${data.tag*}",
        "checkResponseCode" : "true",
        "headers" : "{\"Content-Type\": \"application/json\",\"Authorization\": \"Bearer ${export.cwtoken}\"}",
        "method" : "POST",
        "printResponse" : "true",
        "printResponseToFile" : "false",
        "proxySettings" : "false",
        "remoteUrl" : "https://${export.mgtvip}:30603/crosswork/inventory/v1/tags",
        "responseCode" : "200",
        "sslVerify" : "false",
        "timeout" : "30000"
      },
      "description" : "Create tag in CW",
      "nodeStep" : false,
      "type" : "edu.ohio.ais.rundeck.HttpWorkflowStepPlugin"
    }, {
      "configuration" : {
        "authentication" : "None",
        "body" : "{\"filter\":{},\"filterData\":{\"PageSize\":200,\"PageNum\":0}}",
        "checkResponseCode" : "true",
        "file" : "/opt/cw/tmp/${job.execid}",
        "headers" : "{\"Content-Type\": \"application/json\",\"Authorization\": \"Bearer ${export.cwtoken}\"}",
        "method" : "POST",
        "printResponse" : "true",
        "printResponseToFile" : "true",
        "proxySettings" : "false",
        "remoteUrl" : "https://${export.mgtvip}:30603/crosswork/inventory/v1/tags/query",
        "responseCode" : "200",
        "sslVerify" : "false",
        "timeout" : "30000"
      },
      "description" : "Retrieve tags",
      "nodeStep" : false,
      "type" : "edu.ohio.ais.rundeck.HttpWorkflowStepPlugin"
    }, {
      "description" : "reformat tags",
      "script" : "jq '[.tags[] | {\"name\":(.category+\":\"+.name),\"value\":.name}]' /opt/cw/tmp/@job.execid@ > /opt/cw/options/@option.cnc-pod@-tags.json"
    } ],
    "keepgoing" : false,
    "strategy" : "node-first"
  },
  "uuid" : "801e3747-5afc-4a08-861f-92306ce08eba"
} ]
