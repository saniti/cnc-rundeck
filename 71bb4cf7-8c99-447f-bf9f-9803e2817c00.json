[ {
  "defaultTab" : "nodes",
  "description" : "Create a definition of the L3VPN",
  "executionEnabled" : true,
  "group" : "-DEMO/L3VPN",
  "id" : "71bb4cf7-8c99-447f-bf9f-9803e2817c00",
  "loglevel" : "INFO",
  "name" : "L3VPN Definition - DELETE",
  "nodeFilterEditable" : false,
  "notification" : {
    "onsuccess" : {
      "email" : {
        "recipients" : "device-details@necehealthpoc.local",
        "subject" : "${notification.eventStatus} ${job.name}"
      }
    }
  },
  "notifyAvgDurationThreshold" : null,
  "options" : [ {
    "enforced" : true,
    "hidden" : true,
    "name" : "proxy",
    "required" : true,
    "value" : "proxy:3001",
    "values" : [ "proxy:3001" ],
    "valuesListDelimiter" : ","
  }, {
    "enforced" : true,
    "name" : "existing-vpn",
    "valuesUrl" : "file:/opt/cw/options/vpn-temp.json"
  } ],
  "plugins" : {
    "ExecutionLifecycle" : null
  },
  "scheduleEnabled" : true,
  "sequence" : {
    "commands" : [ {
      "description" : "Delete Entry",
      "script" : "file=/opt/cw/options/vpn-temp.json\nfiletmp=/opt/cw/tmp/vpn-temp.json\n#jq 'del(.[] | select(.value == \"@option.existing-vpn@\"))' /opt/cw/options/dns-servers.json > /opt/cw/tmp/dns-servers.tmp\n#\\cp /opt/cw/tmp/dns-servers.tmp /opt/cw/options/dns-servers.json\n\njq 'del(.[] | select(.value == \"@option.existing-vpn@\"))' $file > $filetmp\n\\cp $filetmp $file\n\n"
    } ],
    "keepgoing" : false,
    "strategy" : "sequential"
  },
  "uuid" : "71bb4cf7-8c99-447f-bf9f-9803e2817c00"
} ]
