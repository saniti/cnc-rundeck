[ {
  "defaultTab" : "nodes",
  "description" : "",
  "executionEnabled" : true,
  "group" : "-DEMO/.working",
  "id" : "150ba2d0-13b8-4c3c-9909-f3a9d47c34dc",
  "loglevel" : "INFO",
  "name" : "CNC - Tunnel usage",
  "nodeFilterEditable" : false,
  "notification" : {
    "onsuccess" : {
      "email" : {
        "recipients" : "device-details@necehealthpoc.local",
        "subject" : "${notification.eventStatus} ${job.name}"
      }
    }
  },
  "notifyAvgDurationThreshold" : null,
  "options" : [ {
    "enforced" : true,
    "label" : "CW POD",
    "name" : "cnc-pod",
    "required" : true,
    "valuesUrl" : "file:/opt/cw/options/cw-environments.json"
  }, {
    "dateFormat" : "x",
    "isDate" : true,
    "name" : "ddtm"
  }, {
    "name" : "colour",
    "value" : "4003"
  }, {
    "label" : "# Days to show",
    "name" : "days",
    "regex" : "\\d+",
    "required" : true,
    "value" : "2"
  }, {
    "label" : "Destination IP",
    "name" : "dest-ip",
    "required" : true,
    "value" : "198.19.1.5"
  }, {
    "label" : "Source IP",
    "name" : "source-ip",
    "required" : true,
    "value" : "198.19.1.3"
  }, {
    "hidden" : true,
    "name" : "template",
    "required" : true,
    "value" : "{\"lspType\":\"SR\",\"peerAddress\":\"198.19.1.5\",\"destAddress\":\"198.19.1.3\",\"from\":\"2023-06-26T22:44:00.802Z\",\"to\":\"2023-06-27T22:44:00.802Z\",\"color\":4003}"
  } ],
  "plugins" : {
    "ExecutionLifecycle" : null
  },
  "scheduleEnabled" : true,
  "sequence" : {
    "commands" : [ {
      "jobref" : {
        "group" : "CNC/Configuration",
        "importOptions" : true,
        "name" : "get key vars",
        "uuid" : "c3b0db66-3fdd-49b7-a5b5-4261a0ff3463"
      }
    }, {
      "configuration" : {
        "debugOnly" : "false"
      },
      "nodeStep" : false,
      "type" : "log-data-step"
    }, {
      "description" : "date manipulation",
      "plugins" : {
        "LogFilter" : [ {
          "config" : {
            "captureMultipleKeysValues" : "true",
            "hideOutput" : "false",
            "logData" : "true",
            "regex" : "^(.+?)\\s*=\\s*(.+)"
          },
          "type" : "key-value-data-multilines"
        } ]
      },
      "script" : "echo \"@option.ddtm@\" | jq -r './1000| gmtime| \"todate=\"+strflocaltime (\"%Y-%m-%dT%H:%M:%SZ\")'\necho \"@option.ddtm@\" | jq -r '(./1000)-(60*60*24*@option.days@)| gmtime| \"fromdate=\"+strflocaltime (\"%Y-%m-%dT%H:%M:%SZ\")'"
    }, {
      "description" : "modify payload",
      "plugins" : {
        "LogFilter" : [ {
          "config" : {
            "invalidKeyPattern" : "\\s|\\$|\\{|\\}|\\\\",
            "logData" : "true",
            "name" : "payload",
            "regex" : "(.*)",
            "replaceFilteredResult" : "false"
          },
          "type" : "key-value-data"
        } ]
      },
      "script" : "template=$(echo '@option.template@' | jq .)\necho $template\necho $template | jq -c '.| (.lspType=\"SR\") | (.color=@option.colour@)| (.peerAddress=\"@option.source-ip@\")| (.destAddress=\"@option.dest-ip@\") | (.from=\"@data.fromdate*@\")| (.to=\"@data.todate*@\")'"
    }, {
      "configuration" : {
        "authentication" : "None",
        "body" : "${data.payload*}",
        "checkResponseCode" : "false",
        "file" : "/opt/cw/tmp/${job.execid}.json",
        "headers" : " {\"Content-Type\": \"application/json\",\"Authorization\": \"Bearer ${export.cwtoken}\"}\n\n",
        "method" : "POST",
        "printResponse" : "true",
        "printResponseToFile" : "true",
        "proxySettings" : "false",
        "remoteUrl" : "https://${export.mgtvip}:30603/crosswork/optima-analytics/api/v1/lsp/utilizations",
        "sslVerify" : "false",
        "timeout" : "30000"
      },
      "description" : "get lsp data",
      "nodeStep" : false,
      "type" : "edu.ohio.ais.rundeck.HttpWorkflowStepPlugin"
    }, {
      "description" : "get CSV",
      "script" : "file=/opt/cw/tmp/@job.execid@.json\n#jq -r '.lsps[]| [ keys[] as $k | .[$k] ]| @csv' $file\n\njq -r '[.[] | .[\"event-time\"] = .tst|.[\"usage\"] = .util  | del(.tst,.util)] | (.[0] | keys_unsorted) as $keys | ([$keys] + map([.[ $keys[] ]])) [] | @csv' $file"
    } ],
    "keepgoing" : false,
    "strategy" : "node-first"
  },
  "uuid" : "150ba2d0-13b8-4c3c-9909-f3a9d47c34dc"
} ]
