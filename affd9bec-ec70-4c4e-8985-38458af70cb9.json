      "script" : "file=/tmp/@job.execid@.json\ntmpfile=/tmp/@job.execid@.json.tmp\n\n#echo '@option.provider-schema@' > $file\nvar=$(echo '@option.provider-schema@' | jq -c .)\nvar=$(echo $var | jq '( .data[].geo_info.coordinates.longitude.value)|=\"@data.location-Longitude*@\"' )\nvar=$(echo $var | jq '( .data[].geo_info.coordinates.latitude.value)|=\"@data.location-Latitude*@\"' )\nvar=$(echo $var | jq '( .data[].geo_info.country)|=\"@data.location-Country*@\"' )\nvar=$(echo $var | jq '(..|objects|select(has(\"inet_addr\"))).\"inet_addr\"|= \"@option.device-ip@\"' )\nvar=$(echo $var | jq '(..|objects|select(has(\"host_name\"))).\"host_name\"|= \"@option.device-name@\"' )  \nvar=$(echo $var | jq '(..|objects|select(has(\"profile\"))).\"profile\"|= \"@option.device-profile@\"' )\nvar=$(echo $var | jq '(..|objects|select(has(\"mask\"))).\"mask\"|= \"@option.device-mask@\"' )\nvar=$(echo $var | jq '(..|objects|select(has(\"provider_node_id\"))).\"provider_node_id\"|= \"@option.device-name-long@\"' )\nvar=$(echo $var | jq '(..|objects|select(has(\"provider_name\"))).\"provider_name\"|= \"@option.nso-provider@\"' )\nvar=$(echo $var | jq '(..|objects|select(has(\"product_info\"))).\"product_info\".\"capability\" |=@data.capabilities*@' )\nvar=$(echo $var | jq '(..|objects|select(has(\"product_info\"))).\"product_info\".\"software_type\" |=\"@option.os-type@\"' )\nvar=$(echo $var | jq '(..|objects|select(has(\"te_router_id\"))).\"te_router_id\"|= \"@option.te_router_id@\"' )\nvar=$(echo $var | jq '(..|objects|select(has(\"telemetry_intf_src_vrf\"))).\"telemetry_intf_src_vrf\"|= \"@option.telemetry_intf_src_vrf@\"' )\nvar=$(echo $var | jq '(..|objects|select(has(\"telemetry_intf_src_name\"))).\"telemetry_intf_src_name\"|= \"@option.telemetry_intf_src_name@\"' )\nvar=$(echo $var | jq '( .data[].connectivity_info[]|select(.type==\"ROBOT_MSVC_TRANS_SNMP\") ).port|=\"@option.device-snmp-port@\"' )\nvar=$(echo $var | jq '( .data[].connectivity_info[]|select(.type==\"ROBOT_MSVC_TRANS_SSH\") ).port|=\"@option.device-ssh-port@\"' )\nvar=$(echo $var | jq '( .data[].connectivity_info[]|select(.type==\"ROBOT_MSVC_TRANS_GNMI\") ).port|=\"@option.gnmi-port@\"' )\nvar=$(echo $var | jq '( .data[].connectivity_info[]|select(.type==\"ROBOT_MSVC_TRANS_NETCONF\") ).port|=\"@option.netconf-port@\"' )\nvar=$(echo $var | jq '.data[].uuid|=\"@data.device-uuid*@\"' )\n\nif [ \"@data.SNMP*@\" ]; then true; else var=$(echo $var | jq 'del(..|objects|select(has(\"type\") and .type==\"ROBOT_MSVC_TRANS_SNMP\"))' ); fi\nif [ \"@data.SSH*@\" ]; then true; else var=$(echo $var | jq 'del(..|objects|select(has(\"type\") and .type==\"ROBOT_MSVC_TRANS_SSH\"))' ); fi\nif [ \"@data.GNMI*@\" ]; then true; else var=$(echo $var | jq 'del(..|objects|select(has(\"type\") and .type==\"ROBOT_MSVC_TRANS_GNMI\"))' ); fi\nif [ \"@data.NETCONF*@\" ]; then true; else var=$(echo $var | jq 'del(..|objects|select(has(\"type\") and .type==\"ROBOT_MSVC_TRANS_NETCONF\"))' ); fi\n\necho $var | jq -c ."
