[ {
  "defaultTab" : "nodes",
  "description" : "Restore a CML topology from source control\n---\n[Update GIT file list](4f9035c3-ed9d-487a-9c3c-4885403b0116)\n\n[CM Authentication](1960ef3e-ddcc-4400-9012-259267f7399b)\n\n[Refresh GIT Repositories](37da7e27-068e-4bac-80b0-bd503f702416)",
  "executionEnabled" : true,
  "group" : "CML",
  "id" : "c6675485-951c-408d-8b72-c84745a7a6fb",
  "loglevel" : "INFO",
  "name" : "CML - Deploy or restore CML topology",
  "nodeFilterEditable" : false,
  "options" : [ {
    "label" : "Topology Name",
    "name" : "name",
    "required" : true
  }, {
    "enforced" : true,
    "label" : "Git Repository",
    "name" : "repo",
    "required" : true,
    "value" : "cml",
    "valuesUrl" : "file:/opt/cw/options/git/repos.json"
  }, {
    "enforced" : true,
    "label" : "Git Branch",
    "name" : "branch",
    "required" : true,
    "value" : "develop",
    "values" : [ "develop", "master", "main" ],
    "valuesListDelimiter" : ","
  }, {
    "enforced" : true,
    "label" : "CML File",
    "name" : "file",
    "required" : true,
    "valuesUrl" : "file:/opt/cw/options/git/${option.repo.value}/${option.branch.value}-files.json"
  }, {
    "hidden" : true,
    "label" : "Current time",
    "name" : "now",
    "required" : true,
    "value" : "${DATE:yyyy-MM-dd-HH-mm-ss}"
  }, {
    "hidden" : true,
    "name" : "git",
    "required" : true,
    "secure" : true,
    "storagePath" : "keys/git/repo",
    "valueExposed" : true
  }, {
    "hidden" : true,
    "name" : "token",
    "required" : true,
    "secure" : true,
    "storagePath" : "keys/gitea/token",
    "valueExposed" : true
  } ],
  "plugins" : {
    "ExecutionLifecycle" : null
  },
  "scheduleEnabled" : true,
  "sequence" : {
    "commands" : [ {
      "description" : "get auth",
      "jobref" : {
        "group" : "CML",
        "name" : "CML - Auth",
        "uuid" : "1960ef3e-ddcc-4400-9012-259267f7399b"
      }
    }, {
      "configuration" : {
        "authentication" : "None",
        "checkResponseCode" : "false",
        "file" : "/opt/cw/tmp/${job.execid}-${option.name}.yml",
        "headers" : "{\"accept\": \"application/json\",\"Authorization\": \"Bearer ${option.token}\"}\n\n",
        "method" : "GET",
        "printResponse" : "true",
        "printResponseToFile" : "true",
        "proxySettings" : "false",
        "remoteUrl" : "${option.file}",
        "sslVerify" : "false",
        "timeout" : "30000"
      },
      "description" : "Get File from GIT",
      "nodeStep" : false,
      "type" : "edu.ohio.ais.rundeck.HttpWorkflowStepPlugin"
    }, {
      "description" : "Set properties from response file",
      "plugins" : {
        "LogFilter" : [ {
          "config" : {
            "captureMultipleKeysValues" : "true",
            "hideOutput" : "false",
            "logData" : "true",
            "regex" : "^(.+?)\\s*=\\s*(.+)"
          },
          "type" : "key-value-data-multilines"
        } ]
      },
      "script" : "file=/opt/cw/tmp/@job.execid@-@option.name@.yml\njq -r '. | to_entries[] | ((\"file-\"+.key|tostring)+\"=\"+(.value|tostring))' $file\necho fdu @data.file-download_url*@"
    }, {
      "configuration" : {
        "authentication" : "None",
        "checkResponseCode" : "false",
        "file" : "/opt/cw/tmp/${job.execid}-${option.name}.yml",
        "headers" : "{\"accept\": \"application/json\",\"Authorization\": \"Bearer ${option.token}\"}\n\n",
        "method" : "GET",
        "printResponse" : "true",
        "printResponseToFile" : "true",
        "proxySettings" : "false",
        "remoteUrl" : "${option.git}$/api/v1/repos/${option.repo}/raw/${data.file-name*}?ref=${option.branch}",
        "sslVerify" : "false",
        "timeout" : "30000"
      },
      "description" : "Get File from GIT",
      "nodeStep" : false,
      "type" : "edu.ohio.ais.rundeck.HttpWorkflowStepPlugin"
    }, {
      "description" : "Convert to JSON",
      "plugins" : {
        "LogFilter" : [ {
          "config" : {
            "invalidKeyPattern" : "\\s|\\$|\\{|\\}|\\\\",
            "logData" : "true",
            "name" : "cml-json",
            "regex" : "(.*)"
          },
          "type" : "key-value-data"
        } ]
      },
      "script" : "file=/opt/cw/tmp/@job.execid@-@option.name@.yml\n(yq -r --prettyPrint  -o=json $file) | jq -c"
    }, {
      "configuration" : {
        "authentication" : "None",
        "body" : "${data.cml-json*}",
        "checkResponseCode" : "false",
        "file" : "/opt/cw/tmp/${job.execid}-${option.name}.yml",
        "headers" : "{\"accept\": \"application/json\",\"Authorization\": \"Bearer ${export.token}\"}\n",
        "method" : "POST",
        "printResponse" : "true",
        "printResponseToFile" : "false",
        "proxySettings" : "false",
        "remoteUrl" : "${export.cml-host}/api/v0/import?title=${option.name}",
        "sslVerify" : "false",
        "timeout" : "30000"
      },
      "description" : "Push to CML",
      "nodeStep" : false,
      "type" : "edu.ohio.ais.rundeck.HttpWorkflowStepPlugin"
    }, {
