        "name" : "get key vars",
        "uuid" : "c3b0db66-3fdd-49b7-a5b5-4261a0ff3463"
      }
    }, {
      "description" : "checkout service",
      "script" : "cd /opt/source\nif [ -d \"/opt/source/l3vpn\" ]; then\n    cd l3vpn\n    git fetch --force\nelse\n    git clone auto@198.18.133.102:dcloud/l3vpn.git -b @option.branch@\nfi\n\nbase=$(jq -c '(..|objects|select(has(\"cvlan-id\"))).\"cvlan-id\" |= @option.vlan-id@' /opt/source/l3vpn/@option.service-name@.json)\nbase=$(echo $base | jq -c '(..|objects|select(has(\"vpn-id\"))).\"vpn-id\" |= \"@option.service-name@\"')\necho $base | jq .\n"
    }, {
      "configuration" : {
        "command" : "curl -k --location --request PATCH 'https://${export.mgtvip}:30603/crosswork/proxy/nso/restconf/data' --header 'Content-Type: application/yang-data+json' --header 'Accept: application/yang-data+json' --header 'Authorization: Bearer ${export.cwtoken}' --data-binary '@/opt/source/l3vpn/${option.service-name}.json'"
      },
      "description" : "post data",
      "nodeStep" : true,
      "type" : "localexec"
    } ],
    "keepgoing" : false,
    "strategy" : "node-first"
  },
  "uuid" : "6576242f-3512-4e90-a824-8c5c64ac542f"
