[ {
  "defaultTab" : "nodes",
  "description" : "Store a CML definition in Git for version control\n---\n[CM Authentication](1960ef3e-ddcc-4400-9012-259267f7399b)\n\n[Refresh GIT Repositories](37da7e27-068e-4bac-80b0-bd503f702416)",
  "executionEnabled" : true,
  "group" : "CML",
  "id" : "18fdd657-b91c-454d-8b47-3a07abcaecd6",
  "loglevel" : "INFO",
  "name" : "CML - Download a lab and store in GIT",
  "nodeFilterEditable" : false,
  "options" : [ {
    "enforced" : true,
    "label" : "Existing Labs",
    "name" : "labs",
    "required" : true,
    "valuesUrl" : "file:/opt/cw/options/cml-labs.json"
  }, {
    "enforced" : true,
    "label" : "Git Repository",
    "name" : "dest-repo",
    "required" : true,
    "value" : "cml",
    "valuesUrl" : "file:/opt/cw/options/git/repos.json"
  }, {
    "enforced" : true,
    "label" : "Git Branch",
    "name" : "branch",
    "required" : true,
    "value" : "develop",
    "values" : [ "develop", "master", "main" ],
    "valuesListDelimiter" : ","
  }, {
    "hidden" : true,
    "label" : "Current time",
    "name" : "now",
    "required" : true,
    "value" : "${DATE:yyyy-MM-dd-HH-mm-ss}"
  } ],
  "plugins" : {
    "ExecutionLifecycle" : null
  },
  "scheduleEnabled" : true,
  "sequence" : {
    "commands" : [ {
      "description" : "get auth",
      "jobref" : {
        "group" : "CML",
        "name" : "CML - Auth",
        "uuid" : "1960ef3e-ddcc-4400-9012-259267f7399b"
      }
    }, {
      "description" : "Find Lab title",
      "plugins" : {
        "LogFilter" : [ {
          "config" : {
            "captureMultipleKeysValues" : "true",
            "hideOutput" : "false",
            "logData" : "true",
            "regex" : "^(.+?)\\s*=\\s*(.+)"
          },
          "type" : "key-value-data-multilines"
        } ]
      },
      "script" : "file=/opt/cw/options/cml-labs.json\ntitle=$(jq -r '.[] | select(.id==\"@option.labs@\").title' $file) \necho lab_title=$title"
    }, {
      "configuration" : {
        "authentication" : "None",
        "checkResponseCode" : "false",
        "file" : "/opt/cw/tmp/${job.execid}-lab-${option.labs}-full.yml",
        "headers" : "{\"accept\": \"application/json\",\"Authorization\": \"Bearer ${export.token}\"}\n\n",
        "method" : "GET",
        "printResponse" : "false",
        "printResponseToFile" : "true",
        "proxySettings" : "false",
        "remoteUrl" : "${export.cml-host}/api/v0/labs/${option.labs}/download",
        "sslVerify" : "false",
        "timeout" : "30000"
      },
      "description" : "Get lab",
      "nodeStep" : false,
      "type" : "edu.ohio.ais.rundeck.HttpWorkflowStepPlugin"
    }, {
      "description" : "Upload file to branch in git",
      "errorhandler" : {
        "jobref" : {
          "args" : "-repo ${option.dest-repo} -branch ${option.branch} -file-name \"${data.lab_title*}.yml\" -file-path /opt/cw/tmp/${job.execid}-lab-${option.labs}-full.yml -message \"Project : ${job.execid},Date : ${option.now}\" ",
          "group" : "git",
          "name" : "git - replace file v2",
          "project" : "Live-2022",
          "uuid" : "cca72684-34dc-4888-ad7c-a7e0787eb1f7"
        },
        "keepgoingOnSuccess" : true
      },
      "jobref" : {
        "args" : "-repo ${option.dest-repo} -branch ${option.branch} -file-name \"${data.lab_title*}.yml\" -file-path /opt/cw/tmp/${job.execid}-lab-${option.labs}-full.yml -message \"Project : ${job.execid},Date : ${option.now}\" ",
        "group" : "git",
        "name" : "git - upload file",
        "project" : "Live-2022",
        "uuid" : "1fe8267f-0bb6-426d-849b-868b06a97950"
      }
    } ],
    "keepgoing" : false,
    "strategy" : "node-first"
  },
  "uuid" : "18fdd657-b91c-454d-8b47-3a07abcaecd6"
} ]
