    }, {
      "description" : "get CSV",
      "script" : "file=/opt/cw/tmp/@job.execid@.json\n#jq -r '.lsps[]| [ keys[] as $k | .[$k] ]| @csv' $file\n\n#jq -r '.\"cisco-crosswork-segment-routing-policy:sr-policies\".policy | del(.. | .\"policy-details\"?)| (.[0] | keys_unsorted) as $keys |   ([$keys] + map([.[ $keys[] ]])) [] | @csv' $file\njq -r '[.\"cisco-crosswork-segment-routing-policy:sr-policies\".policy[]|(..|select(has(\"update-time\"))?).\"update-time\"|=(.|tonumber/1000| gmtime| strflocaltime (\"%Y-%m-%dT%H:%M:%SZ\")) |(..|select(has(\"computed-time\"))?).\"computed-time\"|=(.|tonumber| gmtime| strflocaltime (\"%Y-%m-%dT%H:%M:%SZ\"))| (..|select(has(\"Created\"))?).Created|=(.|tonumber| gmtime| strflocaltime (\"%Y-%m-%dT%H:%M:%SZ\"))|[ [leaf_paths as $path | {\"key\": $path | join(\"_\"), \"value\": getpath($path)}] | from_entries ] []] |  (.[0] | keys_unsorted) as $keys |   ([$keys] + map([.[ $keys[] ]])) [] | @csv' $file"
    } ],
    "keepgoing" : false,
    "strategy" : "node-first"
  },
  "uuid" : "41750681-1c66-4e36-9d35-b419fe57605d"
