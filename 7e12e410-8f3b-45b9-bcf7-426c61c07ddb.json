[ {
  "defaultTab" : "nodes",
  "description" : "",
  "executionEnabled" : true,
  "group" : "CNC/Devices",
  "id" : "7e12e410-8f3b-45b9-bcf7-426c61c07ddb",
  "loglevel" : "INFO",
  "name" : "CNC - Device Mops (all)",
  "nodeFilterEditable" : false,
  "notification" : {
    "onsuccess" : {
      "email" : {
        "recipients" : "device-details@necehealthpoc.local",
        "subject" : "${notification.eventStatus} ${job.name}"
      }
    }
  },
  "notifyAvgDurationThreshold" : null,
  "options" : [ {
    "enforced" : true,
    "label" : "CW POD",
    "name" : "cnc-pod",
    "required" : true,
    "valuesUrl" : "file:/opt/cw/options/cw-environments.json"
  } ],
  "plugins" : {
    "ExecutionLifecycle" : null
  },
  "scheduleEnabled" : true,
  "sequence" : {
    "commands" : [ {
      "jobref" : {
        "group" : "CNC/Configuration",
        "ignoreNotifications" : true,
        "importOptions" : true,
        "name" : "get key vars",
        "uuid" : "c3b0db66-3fdd-49b7-a5b5-4261a0ff3463"
      }
    }, {
      "configuration" : {
        "command" : "echo  ${export.mgtvip} ${export.cwtoken}"
      },
      "nodeStep" : true,
      "plugins" : {
        "LogFilter" : [ {
          "config" : {
            "loglevel" : "error",
            "matchLoglevel" : "normal",
            "quietMatch" : "false",
            "regex" : "((?:\"name\":\")(.*?)(?:\"))"
          },
          "type" : "quiet-output"
        } ]
      },
      "type" : "localexec"
    }, {
      "configuration" : {
        "authentication" : "None",
        "body" : "{\"limit\":300,\"page\":1,\"sort_by\":\"name\",\"order\":\"asc\"}",
        "checkResponseCode" : "false",
        "file" : "/opt/cw/tmp/plays.tmp",
        "headers" : "{\"Content-Type\": \"application/json\",\"Authorization\": \"Bearer ${export.cwtoken}\"}",
        "method" : "POST",
        "printResponse" : "false",
        "printResponseToFile" : "true",
        "proxySettings" : "false",
        "remoteUrl" : "https://${export.mgtvip}:30603/crosswork/nca/v1/plays/query",
        "sslVerify" : "false",
        "timeout" : "30000"
      },
      "description" : "get plays",
      "nodeStep" : false,
      "type" : "edu.ohio.ais.rundeck.HttpWorkflowStepPlugin"
    }, {
      "configuration" : {
        "command" : "jq '[.plays[] | {\"id\":.id,\"name\":.name,\"description\":.description,\"inputs\":.runtime_params}]' /opt/cw/tmp/plays.tmp"
      },
      "description" : "short list",
      "nodeStep" : true,
      "plugins" : {
        "LogFilter" : [ {
          "config" : {
            "datatype" : "application/json",
            "sanitizeHtml" : "true",
            "striped" : "true"
          },
          "type" : "render-datatype"
        } ]
      },
      "type" : "localexec"
    } ],
    "keepgoing" : false,
    "strategy" : "node-first"
  },
  "uuid" : "7e12e410-8f3b-45b9-bcf7-426c61c07ddb"
} ]
