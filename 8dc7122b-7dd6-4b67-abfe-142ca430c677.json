[ {
  "defaultTab" : "nodes",
  "description" : "",
  "executionEnabled" : true,
  "group" : "-DEMO/.working",
  "id" : "8dc7122b-7dd6-4b67-abfe-142ca430c677",
  "loglevel" : "INFO",
  "name" : "CNC - Get LSP changes",
  "nodeFilterEditable" : false,
  "notification" : {
    "onsuccess" : {
      "email" : {
        "recipients" : "device-details@necehealthpoc.local",
        "subject" : "${notification.eventStatus} ${job.name}"
      }
    }
  },
  "notifyAvgDurationThreshold" : null,
  "options" : [ {
    "enforced" : true,
    "label" : "CW POD",
    "name" : "cnc-pod",
    "required" : true,
    "valuesUrl" : "file:/opt/cw/options/cw-environments.json"
  }, {
    "dateFormat" : "x",
    "isDate" : true,
    "name" : "ddtm"
  }, {
    "label" : "# Days to show",
    "name" : "days",
    "regex" : "\\d+",
    "required" : true,
    "value" : "2"
  } ],
  "plugins" : {
    "ExecutionLifecycle" : null
  },
  "scheduleEnabled" : true,
  "sequence" : {
    "commands" : [ {
      "jobref" : {
        "group" : "CNC/Configuration",
        "importOptions" : true,
        "name" : "get key vars",
        "uuid" : "c3b0db66-3fdd-49b7-a5b5-4261a0ff3463"
      }
    }, {
      "configuration" : {
        "debugOnly" : "false"
      },
      "nodeStep" : false,
      "type" : "log-data-step"
    }, {
      "description" : "date manipulation",
      "plugins" : {
        "LogFilter" : [ {
          "config" : {
            "captureMultipleKeysValues" : "true",
            "hideOutput" : "false",
            "logData" : "true",
            "regex" : "^(.+?)\\s*=\\s*(.+)"
          },
          "type" : "key-value-data-multilines"
        } ]
      },
      "script" : "echo \"@option.ddtm@\" | jq -r './1000| gmtime| \"todate=\"+strflocaltime (\"%Y-%m-%dT%H:%M:%SZ\")'\necho \"@option.ddtm@\" | jq -r '(./1000)-(60*60*24*@option.days@)| gmtime| \"fromdate=\"+strflocaltime (\"%Y-%m-%dT%H:%M:%SZ\")'"
    }, {
      "configuration" : {
        "authentication" : "None",
        "body" : "{\"from\":\"${data.fromdate*}\",\"to\":\"${data.todate*}\",\"lspType\":\"SR\",\"startRow\":0,\"endRow\":30,\"sortColumn\":\"totalEventsCount\",\"sortAscending\":false,\"filterCriteria\":{\"type\":\"and\",\"conditionList\":[{\"type\":\"equals\",\"name\":\"lspType\",\"value\":\"SR\"}]}}",
        "checkResponseCode" : "false",
        "file" : "/opt/cw/tmp/${job.execid}.json",
        "headers" : " {\"Content-Type\": \"application/yang-data+json\",\"Authorization\": \"Bearer ${export.cwtoken}\"}\n\n",
        "method" : "POST",
        "printResponse" : "true",
        "printResponseToFile" : "true",
        "proxySettings" : "false",
        "remoteUrl" : "https://${export.mgtvip}:30603/crosswork/optima-analytics/api/v1/dashboard/lsp/events",
        "sslVerify" : "false",
        "timeout" : "30000"
      },
      "description" : "get lsp data",
      "nodeStep" : false,
      "type" : "edu.ohio.ais.rundeck.HttpWorkflowStepPlugin"
    }, {
      "description" : "get CSV",
      "script" : "file=/opt/cw/tmp/@job.execid@.json\n#jq -r '.lsps[]| [ keys[] as $k | .[$k] ]| @csv' $file\n\n#jq -r '[.lsps[]  |[ keys[] as $k | .[$k]]] | [[\"color\",\"head-end\",\"tail-end\",\"id\",\"lsp-type\",\"metric-type\",\"change-count\",\"path-change-count\",\"total\"]]+. | .[] | @csv ' $file\njq -r '[.lsps[] | [ [leaf_paths as $path | {\"key\": $path | join(\"_\"), \"value\": getpath($path)}] | from_entries ] []] |  (.[0] | keys_unsorted) as $keys |   ([$keys] + map([.[ $keys[] ]])) [] | @csv' $file"
    } ],
    "keepgoing" : false,
    "strategy" : "node-first"
  },
  "uuid" : "8dc7122b-7dd6-4b67-abfe-142ca430c677"
} ]
